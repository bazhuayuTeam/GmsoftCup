@CHARSET "UTF-8";
.pager{
	background:#fafafa;
	font-family:"宋体";
	height:30px;
	width:auto;
	border-left:1px solid #afafaf;
	border-right:1px solid #afafaf;
	border-bottom:1px solid #afafaf;
	text-align:center;
	font-size:12px;
}
.pager img{
	vertical-align:middle;
	cursor:pointer;
}
.turnPage{
	font-size:12px;
	color:#333;
	line-height:30px;
}
.pageNumber{
	margin:0 	5px;
}
.jump{
	margin-left:10px;
	color:#8b898b;
}
.inputPage{
	height:16px;
	width:30px;
	margin-right:5px;
	text-indent:5px;
}
.sumPage{
	color:#8b898b;
}

.count{
	float:right;
	color:#3366ff;
	padding-right:10px;
	letter-spacing: 2px;
}
