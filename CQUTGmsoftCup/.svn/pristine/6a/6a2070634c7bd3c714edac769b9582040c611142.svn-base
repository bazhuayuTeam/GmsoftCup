
body{
	background:#f6f6f6 none repeat scroll 0 0;
	overflow:hidden;
}
.frame{
    width:100%;
    background:#FFFFFF;
    position:fixed;
    z-index:2;
    border-bottom:1px solid #c5c5c5;
}
.inputSearchList{
	margin-top:25px;
	margin-left:0;
	padding-left:18px;
}
.searchSelect{
	height:30px;
	margin-top:17px;
	width:190px;
}
.searchName{
	line-height:63px;
}
#MainArea{
   margin-top:150px;
   margin-left:20px;
   height:300px;
   width:780px;
   position:absolute;
}
#dataArea{
   width:455px;
   margin-left:190px;
   margin-top:465px;
   position:absolute;
   padding-bottom:30px;
   
}

.head{
    color:#333333;
	font-size:15px;
    padding:23px 18px;
	height:15px;
	text-align:center;
	cursor:pointer;
	float:left;
	position:relative;
	font-family:Microsoft YaHei;
}
a {
	text-decoration: none;
}
.promptDiv{
	padding-right:20px;
}
.promptPic{
	margin-bottom:-2px;
	margin-right:10px;
}
