    .pic-item{
		float:left;
		height:178px;
		width:151px;
		text-align:center;
		background-color:white;
		margin-left:6px;
		border:1px solid #ededee;
		box-shadow: 2px 2px 5px #9e9b9b;
		margin-top : 6px;  
   }
   
.pic-item img{
	width: 148px;
	margin : 0 auto;
	height : 155px;
	border:0;
}

.pic-item span{
	display: block;
	width:148px;
	overflow: hidden;
	height : 2em;
	white-space:nowrap;
	text-overflow:ellipsis;
	-o-text-overflow:ellipsis;
	color:#3b3b3b;
}

.pic-item span,.pic-item a {
	line-height: 2em;
	text-decoration: none;
}

.pic-item a:HOVER span{
	text-decoration: underline;
	color:red;
}

.pic-item a:HOVER img{
	opacity : 0.8;
}

#pager{
	clear: both;
}

