	.news{
	    width: 682px;
	    border: 0px;
	    margin:0 auto;
	    font: normal;
	    margin-top:10px;
	    font-size: 12px;
	    /*text-align: justify;*/
	    word-break: break-all; 
	    word-wrap:break-word;
	    text-justify :distribute-all-lines;
	    height:auto;

	}
	
	.news *{
		max-width: 682px;
	}
	.news-list{
		list-style:none;
		padding-left:0px;
		padding-top : 0px;
		position: relative;
	}
	
	.news .news-list li{
		height:25px;
		line-height:26px;
		font-size:12px;
		border-bottom:1px dashed #c3c2c2;
		position: relative;
	}
	.news .news-list li a{
		display:block;
		overflow:hidden; 
		color:#3a3b3b;
		width:560px;
		height:25px;
		white-space:nowrap;
		text-overflow:ellipsis;
		text-decoration:none;
		float:left;
	}
	.news .news-list li a:hover{
		text-decoration:underline;
		color:#f90415;
	}
	
	.news .news-list li span.news-time
	{
		line-height:26px;
		height:25px;
		float:right;
		text-align:right;
	}
	
