@charset "utf-8";
.allBody{
	overflow:hidden;
	position: relative;
	margin: 0px;
}
/*背景图片*/
.bodyImg{
	width: 100%;
	height: 100%;
	position: absolute;
	min-width: 1366px;
}

/*遮罩*/
.mask{
	background-color:#2e2e2e;
	filter:alpha(opacity=97); 
	-moz-opacity:0.97; 
	opacity:0.97;
}

/*-----------------------------中间登陆块----------------------------------------*/
.login{
	position: relative;
	width:100%;
	height: 70%;
	min-width: 1366px;
	min-height:450px;
}

/*左边*/
.leftLogin{
	width: 32%;
	height: 100%;
	min-height:450px;
	min-width:400px;
	float: left;
}

.loginMask{
	width: 18%;
	height: 100%;
	float: left;
}

.leftFilter{
	background-color:#8ab939;
	filter:alpha(opacity=80); 
	-moz-opacity:0.8; 
	opacity:0.8;
}

.cqutLogin{
	width: 100%;
	height: 60%;
	text-align:center;
}

.loginOfThe{
	width: 100%;
	height: 40%;
	text-align:center;
}

/*右边*/
.loginTitle{
	width: 100%;
	height: 10%;
	padding-top:7%;
}

.loginFontStyle{
	font-family: "Arial";
	font-size: 24px;
	color:#aecd80;
	margin-left: 10%;
	margin-top: 10%;
}

.inputOfLogin{
	width: 100%;
	height: 35%;
}

.inputStyle{
	border: none;
	border-bottom: 1px solid #cbcaca;
	margin-left: 10%;
	width: 80%;
	margin-top: 4%;
	font-family: "微软雅黑";
	font-size: 16px;
	color:#cbcaca;
	height: 35px;
	background-color: ##ffffff;
	background: white;
}

.loginBotton{
	width: 80%;
	margin-left: 10%;
}

.loginBotton a{
	border-top-right-radius:50px;
	border-bottom-right-radius:50px; border-bottom-left-radius :50px;
	border-top-left-radius :50px;
/*	height: 50px;*/
	border: 1px solid #8ab939;
	cursor:pointer;
	width:100%;
	padding:0.8em;
	text-decoration:none;
	width: 90%;
	text-align: center;
	font-family: "Microsoft YaHei";
	font-size:18px;
	color: #8ab939;
}

.roleChoose{
	margin-top: 8%;
	margin-left: 10%;
	width: 100%;
}

.roleChoose label{
	font-family: "Microsoft YaHei";
	font-size: 14.16px;
	color: #333333;
	margin-left: 8px;
	background: url(../../images/login/checkbox.png) no-repeat left center;
	cursor: pointer;
	padding-left: 28px;
}

.roleChoose table{
	width: 91%;
	margin-left:-11px;
}

.roleChoose td{
	text-align: left;
	width: 27%;
	height:28px;
}

.roleChoose input{
	display:none;
	cursor:pointer;
	border: 1px solid #cbcaca;
	margin: 3px 3px 0px 5px;;
}
.labelchecked {
  background: url(../../images/login/selected.png) no-repeat left center;
}

/*提示登陆错误*/
.loginError{
	margin-left:10%;
	margin-top: 3%;
}

.loginError span{
	font-family: "微软雅黑";
	font-size: 12px;
	color: #f77636;
}

.imgInput{
	position: absolute;
	margin-top: 23px;
	margin-left: -30px;
}

.loginFont{
	margin-left:45%;
	font-size:18px;
	margin-top:12px;
	font-size: "Microsoft YaHei";
	color: #8ab939;
	width:20px;
	float: left;
}

/*---------------------------登陆效果-----------------------------------*/
.hvr-rectangle-out {
  display: inline-block;
  vertical-align: middle;
  -webkit-transform: translateZ(0);
  transform: translateZ(0);
  box-shadow: 0 0 1px rgba(0, 0, 0, 0);
  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
  -moz-osx-font-smoothing: grayscale;
  position: relative;
  -webkit-transition-property: color;
  transition-property: color;
  -webkit-transition-duration: 0.3s;
  transition-duration: 0.3s;
}
.hvr-rectangle-out:before {
  content: "";
  position: absolute;
  z-index: -1;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: #8ab939;
  filter:alpha(opacity=80); 
  -moz-opacity:0.8; 
  opacity:0.8;
  -webkit-transform: scale(0);
  transform: scale(0);
  -webkit-transition-property: transform;
  transition-property: transform;
  -webkit-transition-duration: 0.3s;
  transition-duration: 0.3s;
  -webkit-transition-timing-function: ease-out;
  transition-timing-function: ease-out;
  border-top-right-radius:50px;
	border-bottom-right-radius:50px; border-bottom-left-radius :50px;
	border-top-left-radius :50px;
}
.hvr-rectangle-out:hover, .hvr-rectangle-out:focus, .hvr-rectangle-out:active {
  color: white;
}
.hvr-rectangle-out:hover:before, .hvr-rectangle-out:focus:before, .hvr-rectangle-out:active:before {
  -webkit-transform: scale(1);
  transform: scale(1);
}
