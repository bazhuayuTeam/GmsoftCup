@CHARSET "UTF-8";
.center {
	height:500px;
}

.center .input{
	margin-top:-40px;
    height: 40px;
    line-height: 40px;
    width: 320px;
    display: block;
  	clear: right;
  	float: right;
}

.center li {
	margin-top: 12px;
}
.label{
	display: block;
    margin-right: 10px;
    height: 40px;
    line-height: 40px;
    text-align: right;
    width:90px;
    clear: left;
}

.center select {
	display: block;
  	clear: right;
  	float: right;
  	margin-top:-40px;
}

.submit {
	width:332px;
	float: right;
	background-color: #52acea;
    border: 1px solid #52acea;
}