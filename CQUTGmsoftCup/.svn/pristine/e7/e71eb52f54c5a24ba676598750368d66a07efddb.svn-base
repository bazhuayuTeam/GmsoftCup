@charset "utf-8";
/* CSS Document */
* {padding:0;margin:0;}
a {text-decoration:none; color:#333333; cursor:pointer;}

.allContent{
	width:100%;
	margin-left:20px;
	overflow-y:hidden;
	overflow-x:hidden;
}

.addnew{
	margin-left:20px;
	width:100%;
}

.table-content tr{
	width:auto;
	height:30px;
	font-family:"宋体";
	font-size:13px;
	color:#33333;
	line-height:30px;	
}

.table-content tr td{
	border-right:1px solid #c3a080;
	border-bottom:1px solid #c3a080;		
}

.table tr td{
	border-right:1px solid #c3a080;
	border-bottom:1px solid #c3a080;	
	font-family:宋体;
	font-size:13px;
	text-align:center;
	color:#c3a080;	
}

.table-th{
	text-align:center;
	text-indent:5px;
	height:30px;
	background-color:#ececec;
	font-family:宋体;
	font-size:13px;
	border-top:1px solid #c3a080;
}

.table-td{
	text-align:left;
	text-indent:5px;
	height:100px;
	width:15%;
}

.table-td input{
	border:0;
	height:30px;
	width:94%;	
}

.save-button-noexplain{
	height:30px;
	width:70px;
	margin-top:10px;	
}


/*优秀校友*/
.tableStyle{
	border-top:1px solid #c3a080;
}

.tableStyleNew{
	border-left:1px solid #c3a080;
}

.tableinput{
	border:0;
	width:99%;
	height:30px;
	line-height:30px;
	text-indent:5px;
}

.stuCard{
	height:40px;
	width:100%;
	background-color:#49a2f6;
	line-height:40px;
	font-family:微软雅黑;
	font-size:15px;
	color:#ffffff;
}

.operatortext{
	margin-right:10px;
	font-family:'宋体';
	font-size:13px;
	color:#fff;
	cursor:pointer;
}

img.image{
	vertical-align:text-top;
	margin-right:5px;
}

.showCenter{
	position:relative;
	float:right;
	margin-top:-25px;
	margin-right:50%;
	cursor:pointer;
}

.showName{
	margin-left:20px;
}

/*质量监控*/
.chooseTitle{
	width:4%;
	border:1px solid #c3a080;
	border-right:0;
	height:399px;
}

.qualityWarn{
	background-color:#3289db;
	height:131px;
	border-bottom:1px solid #cfcfcf;
	text-align:center;
	cursor:pointer;
}

.showText{
	margin:0 auto;
	padding-top:29px;
	width:20px;
	line-height:18px;
	font-family:微软雅黑;
	font-size:16px;
	color:#fff;
}

.notdisplayText{
	margin:0 auto;
	padding-top:29px;
	width:20px;
	line-height:18px;
	font-family:微软雅黑;
	font-size:16px;
	color:#333333;
}

.contentText{
	width:96%;
	border:1px solid #afafaf;
	border-left:0;
	height:399px;
	float:right;
	margin-top:-401px;
}

.area{
	width:98.1%;
	border:0;
	height:389px;
	resize:none;
	font-family:'宋体';
	font-size:13px;
	padding-top:10px;
	padding-left:10px;
	overflow-y:auto;
}

.qualityArea{
	width:98%;
	height:267px;
	resize:none;
	border:0;
	font-family:'微软雅黑';
	font-size:13px;
}