#pager ul.pages{display: block;border: none;text-transform: uppercase;font-size: 10px;padding: 0;margin:0 auto;text-align: center;}
#pager ul.pages li{list-style: none;border: 1px solid #ccc;text-decoration: none;margin: 0 5px 0 0;padding: 5px;font-size:12px;display: inline;position: relative;}
#pager ul.pages li:hover{border: 1px solid #003f7e;}
#pager ul.pages li.pgEmpty{border: 1px solid #eee;color: #eee;}
#pager ul.pages li.pgCurrent{border: 1px solid #003f7e;color: #000;font-weight: 700;background-color: #eee;}
#pager ul.pages li{width:25px;}